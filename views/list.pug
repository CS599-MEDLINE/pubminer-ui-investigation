//-
   list.pug

   Display the list view.  The table displays search results.

extends layout.pug
block scripts
    //- bower_components should be moved to a vendor script directory
    script(src="bower_components/datatables/media/js/jquery.dataTables.min.js")
    script(src="bower_components/patternfly/dist/js/patternfly-functions-data-tables.js")
    script(src="scripts/wntable.js")
    
block content
    div(id="whatsnew" style="text-align: center")
        h2 What's New in Our Database
        h3 Last update 4/4/2018 
        h4(id="update-count")

    table(class="table table-striped table-border table-hover" id="table1")
        thead
            tr
                th PMC ID
                th Title
